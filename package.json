{
  "name": "l4n-server-voice-ts3",
  "version": "1.0.0-rc.1",
  "description": "automatic setup voice channel for lobbies and add a link to join them to the lobby screen",
  "main": "index.js",
  "scripts": {
    "lint": "eslint --ignore-path .gitignore --ext js,jsx  .",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Bonuspunkt",
  "license": "MIT",
  "dependencies": {
    "node-teamspeak-api": "^1.0.0"
  },
  "peerDependencies": {
    "l4n-server": "*",
    "react": "*",
    "proptypes": "*"
  },
  "devDependencies": {
    "eslint": "4.8.0",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-react": "^7.4.0",
    "prettier": "^1.7.4"
  },
  "eslintConfig": {
    "env": {
      "es6": true,
      "node": true,
      "mocha": true
    },
    "parserOptions": {
      "ecmaVersion": 2017,
      "ecmaFeatures": {
        "experimentalObjectRestSpread": true
      }
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "plugins": [
      "prettier",
      "react"
    ],
    "rules": {
      "no-unused-vars": [
        "error",
        {
          "vars": "all",
          "args": "none"
        }
      ],
      "prettier/prettier": [
        "error",
        {
          "printWidth": 100,
          "singleQuote": true,
          "tabWidth": 4,
          "trailingComma": "all"
        }
      ],
      "react/prop-types": [
        "warn"
      ]
    }
  },
  "directories": {
    "lib": "lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Bonuspunkt/l4n-server-voice-ts3.git"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/Bonuspunkt/l4n-server-voice-ts3/issues"
  },
  "homepage": "https://github.com/Bonuspunkt/l4n-server-voice-ts3#readme"
}
